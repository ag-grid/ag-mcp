{
  "name": "ag-grid-mcp",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "documentation/ag-grid-mcp/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "documentation/ag-grid-mcp/dist",
        "tsConfig": "documentation/ag-grid-mcp/tsconfig.json",
        "packageJson": "documentation/ag-grid-mcp/package.json",
        "main": "documentation/ag-grid-mcp/src/index.ts",
        "assets": ["documentation/ag-grid-mcp/*.md"]
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsx src/index.ts",
        "cwd": "documentation/ag-grid-mcp"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../coverage/documentation/ag-grid-mcp"
      }
    }
  },
  "tags": ["scope:documentation", "type:library"]
}